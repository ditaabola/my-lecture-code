package lv.dita;

import java.util.Arrays;
import java.util.Scanner;

public class TestScannerInputArray {

	public static void main(String[] args) {

String[] strArrStrings = new String[3];
int [] numberOfNames;
Scanner sc = new Scanner(System.in);
System.out.println(strArrStrings.length + " users in array:");

numberOfNames = new int[sc.nextInt()];

int i = 0;
while(i>numberOfNames.length){
	i++;
	System.out.println("Enter a name: "+i);
}

if(numberOfNames.length == 0);
	
System.out.println("EXIT");

	}

}
