
public class ArraysIfElse {

	public static void main(String[] args) {
		int[] number = {1, 2, 3, 4, 5, 6};
		for(int i = 0; i<number.length; i++){
		int checkNumber = 5;
			if(number[4] == checkNumber){
			System.out.println("ONE");
		}else{
			System.out.println("Does not match");
			
			}
			}
	}

}
