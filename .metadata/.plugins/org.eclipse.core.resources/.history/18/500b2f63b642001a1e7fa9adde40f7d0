package majas.darbs;

public class TestEmployee {
	public static void main(String[] args) {
		
		Employee johnDoe = new Employee(01, "John", "Doe", 1000);
			System.out.println("We have a new employee in our company!");
			System.out.println(johnDoe.toString());
		johnDoe.raiseSalary(0.3);
	
	
		Employee maryJane = new Employee(02, "Jane", "Mary", 1000);
		System.out.println("We have a new employee in our company!");
		System.out.println(maryJane.toString());
		
		}
}
