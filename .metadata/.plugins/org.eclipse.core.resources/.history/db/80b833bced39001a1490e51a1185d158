public class ArraysPrimitiveDataType {

	public static void main(String[] args) {
		int[] arrays; // Declares array with type Integer, mostly used
		// int arrays2[]; Same, rarely used
		arrays = new int[2]; // tiek pieskirtas divas jaunas vertibas (ir tagad
								// 0, 1, 2)

		int[] arr = new int[10];
		int[] arrTest = new int[1];
		arrTest[0] =10;
		System.out.println(arrTest[0]);
	}

}
