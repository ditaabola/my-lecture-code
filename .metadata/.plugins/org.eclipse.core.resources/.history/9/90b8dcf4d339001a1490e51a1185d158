
public class Calculator {

	
	public static void main(String[] args) {
		double num = 4.50d; 
		double num1= 2.0d;
		char c ='+';
		if(c =='+'){
			System.out.println(num+num1);
		}else if(c =='-'){
			System.out.println(num-num1);
		}else if(c =='*'){
			System.out.println(num*num1);
		}else if(c =='/'){
			System.out.println(num/num1);
		}else if(c =='%'){
		}
		else{
			System.out.println("WRONG CALCULUS");
		}
	}

}
