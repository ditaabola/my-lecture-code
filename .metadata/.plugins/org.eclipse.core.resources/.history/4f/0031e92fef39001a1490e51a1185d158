public class ArraysPrimitiveDataType {

	public static void main(String[] args) {
		int[] arrays; // Declares array with type Integer, mostly used
		// int arrays2[]; Same, rarely used
		arrays = new int[2]; // tiek pieskirtas divas kastites - [0] un [1]

		int[] arr = new int[10];
		int[] arrTest = new int[5];
		arrTest[0] = 10;
		arrTest[1] = 3;
		arrTest[4] = 4;
			System.out.println(arrTest[0] + " " + arrTest[1]);
		
	}

}
