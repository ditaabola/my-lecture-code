package lv.dita.inputoutput;

import java.io.File;
import java.io.FileWriter;
import java.util.ArrayList;
import java.util.Arrays;

public class TestChangeOneString {

	public static void main(String[] args) throws Exception{
	String string = "Hello user";
	String lastString = string.replace("userName", " ");
			
	System.out.println(lastString+"Dita");

	
	File fileObj = new File("newfile.txt");
	FileWriter fw = new FileWriter(fileObj);
	fw.write(string);


	
	
	
	}

}
