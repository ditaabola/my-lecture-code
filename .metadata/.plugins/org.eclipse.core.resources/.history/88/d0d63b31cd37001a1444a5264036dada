
public class TaskVar {

	public static void main(String[] args) {
		int numAdd;
		numAdd= 125 + 24;
		System.out.println(numAdd +"=125+24");
		int numSubstract;
		numSubstract = 125-24;
		System.out.println(numSubstract + "=125-24");
		int numMult;
		numMult = 125 * 24;
		System.out.println(numMult +"= 125*24");
		int numDivide;
		numDivide = 125/24;
		System.out.println(numDivide + " = 125/24");
	}

}
