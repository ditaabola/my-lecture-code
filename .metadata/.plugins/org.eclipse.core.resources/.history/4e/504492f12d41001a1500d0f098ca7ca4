package lv.dita;

public class TestBookStore {

	public static void main(String[] args) {
char m = 'm';
char f = 'f';
char o = 'u';

if(m == o || f == o){
	System.out.println("It is M or F");
}else{
	System.out.println("Not a correct gender");
}

switch(o){
case 'm':
	System.out.println("M");
	break;
case 'f':
	System.out.println("F");
	break;
default:
	System.out.println("Not a correct gender ");
}
		
	}

}
