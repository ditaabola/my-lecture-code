package lv.dita;

import java.util.Arrays;
import java.util.Scanner;

public class TestScannerInputArray {

	public static void main(String[] args) {

String [] strArrStrings;
int [] numberOfNames;
Scanner sc = new Scanner(System.in);
System.out.println("Enter number of names: ");

numberOfNames = new int[sc.nextInt()];

while(numberOfNames.length>0){
int i;
i = numberOfNames.length;
	System.out.println("Enter a name: "+i;)
	i++;
}
if(numberOfNames.length == 0);
System.out.println("EXIT");
System.out.println(Arrays.toString(strArrStrings));
	}

}
