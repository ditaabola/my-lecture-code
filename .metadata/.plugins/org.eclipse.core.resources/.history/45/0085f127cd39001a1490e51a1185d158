
public class IfElseTasks {

	 
	public static void main(String[] args) {
		int time = 4;
		if(time < 10){
			if(time > 6 && time < 10){
			System.out.println("Morning")
			if(time<6)}
			System.out.println("Go to sleep");
		}
		}else{
			;
		}		
		}else{
			System.out.println("Good day");
		}
	}

}
