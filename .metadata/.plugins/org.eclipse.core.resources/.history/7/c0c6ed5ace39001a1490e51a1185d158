
public class IfElseIfTasks {

	public static void main(String[] args) {
		
		int time = 8;
		
		if(time <= 8){
			System.out.println("Good morning.");
		
		}else if(time <= 18){
			System.out.println("Good day.");
		}else if(time<5){
			System.out.println("Good night");
		}else{
			System.out.println("Good evening.");
		}
	}

}
